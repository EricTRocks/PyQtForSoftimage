<?xml version="1.0" encoding="utf-8"?>
<registry name="[module]" version="1.0">
	<module>[package]</module>
	<created>[createdBy]</created>
	<views>
		<view group="[group]" name="[name]" class="[class]">
			<desc>[desc]</desc>
			<icons>
				<icon scheme="default" loc="icons/default.png"/>
			</icons>
		</view>
	</views>
	<configs>
		<config group="Views" name="[name]" class="[config]">
			<desc>Sets the preferences for the [name] view</desc>
			<icons>
				<icon scheme="default" loc="icons/default.png"/>
			</icons>
		</config>
	</configs>
</registry>